{
    "$schema": "https://raw.githubusercontent.com/WhyNotDogie/tstmlog/master/tsconfig-schema.json",
    "compilerOptions": {
        "target": "ESNext",
        "lib": ["ESNext"],
        "moduleResolution": "Node",
        "types": ["ts-mlog-types"],
        "strict": true,
    },
    "tstmlog": {
        "isWorld": true,
        "entryPoint": "src/index.ts",
        "mlogTarget": "v8",
    }
}